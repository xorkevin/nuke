{
  "name": "@xorkevin/nuke",
  "version": "0.1.29",
  "description": "a reactive ui toolkit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xorkevin/nuke.git",
    "directory": "packages/nuke"
  },
  "author": "Kevin Wang <kevin@xorkevin.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/xorkevin/nuke/issues"
  },
  "homepage": "https://github.com/xorkevin/nuke#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./dist/lib/index.js",
    "./component/*": "./dist/lib/component/*/index.js",
    "./*": "./dist/lib/*.js"
  },
  "scripts": {
    "build": "tsc --build && node scripts/build.js",
    "build:dev": "tsc --build && node scripts/build.js",
    "lint": "eslint . && prettier --ignore-path ../../.prettierignore --check .",
    "format": "prettier --ignore-path ../../.prettierignore --write .",
    "clean": "rm -rf dist .tsbuildinfo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.0.3",
    "@eslint/js": "^8.40.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "typescript-plugin-css-modules": "^5.0.1"
  },
  "peerDependencies": {
    "@types/react": ">=18.0.0",
    "react": ">=18.0.0"
  }
}
